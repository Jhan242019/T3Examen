@{ List<DetalleRutina> det = ((List<DetalleRutina>)Model); }

<body>
    <header>
        <div class="container">
            <h1 class="h-100 badge badge-dark">Usuario: @User.Claims.FirstOrDefault().Value</h1>
        </div>
    </header>
</body>

<h1>Lista de Ejercicios</h1>

<table class="table">
    <thead class="thead-dark">
        <tr>
            <th>Ejercicio </th>
            <th>Video</th>
            <th>Tiempo del Ejercicio </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in det)
        {
            <tr>
                <td>@item.Ejercicios.Nombre</td>
                <td>@item.Ejercicios.Video</td>
                <td>@item.Tiempo</td>
            </tr>
        }
    </tbody>
</table>